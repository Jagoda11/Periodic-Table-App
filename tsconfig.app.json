{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/**/*",
    "env.d.ts",
    "src/typings",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.vue",
    "src/**/*.json",
    "src/**/*.d.ts",
    "src/types/ElementData.ts",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "tests/**/*.js",
    "tests/**/*.vue",
    "playwright-tests/**/*.ts",
    "playwright.config.ts",
    "vite.config.ts",
    "jest.config.ts",
    "nightwatch.conf.js",
    "playwright-tests/playwright.config.ts"
  ],
  "exclude": ["node_modules", "dist", "public", "cypress"],
  "compilerOptions": {
    "esModuleInterop": true,
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["jest"]
  }
}
